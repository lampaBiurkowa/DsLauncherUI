{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "dibstore-app",
  "version": "0.0.0",
  "identifier": "com.dibrysoft.launcher",
  "plugins": {
    "cli": {
      "args": [
        {
          "name": "port"
        }
      ]
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Dibrysoft Launcher",
        "width": 1280,
        "height": 720,
        "minWidth": 1000,
        "minHeight": 500,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
