{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "dibstore-app",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "scope": ["**", "$APPDATA/*"],
        "all": true
      },
      "all": false,
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "binaries/ndib-get",
            "sidecar": true,
            "args": true
          }
        ]
      },
      "window": {
        "startDragging": true,
        "minimize": true,
        "maximize": true,
        "unmaximize": true,
        "close": true
      },
      "dialog": {
        "open": true
      },
      "protocol": {
        "asset": true
      }
    },
    "bundle": {
      "active": true,
      "externalBin": ["binaries/ndib-get"],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.dibrysoft.launcher",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Dibrysoft Launcher",
        "width": 1280,
        "height": 720,
        "minWidth": 1000,
        "minHeight": 500,
        "decorations": false
      }
    ]
  }
}
